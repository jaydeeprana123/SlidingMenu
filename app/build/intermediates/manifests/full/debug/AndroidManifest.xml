        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="info.androidhive.slidingmenu.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="info.androidhive.slidingmenu.NavigationActivity" />
        <activity android:name="info.androidhive.slidingmenu.MenuListActivity" />
        <activity android:name="info.androidhive.slidingmenu.MenuItemListActivity" />
        <activity android:name="info.androidhive.slidingmenu.MenuItemDetailActivity" />
